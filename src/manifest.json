{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "cranmo - R CRAN Package Description Mouseover",
  "short_name": "cranmo",
  "description": "Fetches and displays descriptions of CRAN package links on mouseover",
  "version": "0.01",

  "author": "Altan Orhon",
  "homepage_url" : "https://github.com/orhmeh09/cranmo",
  "icons": {
    "64": "cranmo64.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*.r-project.org/*", "*://*.rstudio.com/*", "*://*.r-bloggers.com/*", "*://*.r-pkg.org/*"],
      //"matches": [ "<all_urls>"],
      "js": ["jquery-3.2.1.min.js", "jquery.hoverintent.js", "content.js"]
    }
  ]
}